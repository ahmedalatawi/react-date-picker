{
  "name": "@atawi/react-date-picker",
  "version": "1.0.0",
  "description": "A beautiful, customizable date and time picker component for React with multiple themes and localization support",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "ts": "yarn tsc --project tsconfig.app.json --noEmit --watch",
    "coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build",
    "docs:dev": "vite --config vite.docs.config.ts",
    "docs:build": "vite build --config vite.docs.config.ts",
    "docs:preview": "vite preview --config vite.docs.config.ts"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "date-fns": "^3.3.1",
    "lucide-react": "^0.358.0",
    "react-popper": "^2.3.0",
    "react-router-dom": "^6.22.3",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.71.1",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^1.3.1"
  },
  "keywords": [
    "react",
    "date-picker",
    "time-picker",
    "datetime-picker",
    "calendar",
    "datepicker",
    "timepicker",
    "react-component",
    "typescript",
    "css",
    "scss",
    "accessibility",
    "internationalization",
    "responsive",
    "customizable",
    "themes"
  ],
  "author": "Ahmed Alatawi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmedalatawi/react-date-picker"
  },
  "bugs": {
    "url": "https://github.com/ahmedalatawi/react-date-picker/issues"
  },
  "homepage": "https://github.com/ahmedalatawi/react-date-picker#readme"
}
